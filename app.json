                "NSLocationWhenInUseUsageDescription": "This app uses the location to find the Cabs near you.",
                "NSCameraUsageDescription": "This app uses the camera to take your profile picture.",
                "NSPhotoLibraryUsageDescription": "This app uses Photo Library to help upload your profile picture.",
                "UIBackgroundModes": [
                    "location",
                    "fetch"
                ]
            },
            "config": {
                "googleMapsApiKey": "AIzaSyB2tjARQd4izKAbLIggavmCZsykqn1M13Q"
            }
        },
        "android": {
            "package": "com.app.uderdriverapp",
            "versionCode": 2,
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE",
                "LOCATION",
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "CAMERA_ROLL"
            ],
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyB2tjARQd4izKAbLIggavmCZsykqn1M13Q"
